{
  "name": "next-js-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:statics": "next build && tsc --project tsconfig.server.json && cp -r public dist/public",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "format": "prettier --write '**/*.ts' **/*.tsx"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "client-sessions": "^0.8.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "morgan": "^1.9.1",
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "@types/client-sessions": "^0.8.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.8.4",
    "babel-plugin-module-resolver": "4.0.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "next-with-typescript": "^1.0.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.3",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "ts-node": "^8.4.1",
    "typescript": "3.7.4"
  }
}
